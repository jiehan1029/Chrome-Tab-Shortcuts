{
  "name": "Tab Shortcuts",
  "description": "manage tabs with keywords and snippets",
  "version": "1.0",
  "author": "jiehan1029@gmail.com",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage", "contextMenus"],
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "browser_action": {
  	"default_title": "Tab Shortcuts",
    "default_popup": "popup.html",
    "default_icon": {
    	"16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png",
      "128": "icon128.png"
  	}
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "newTab.html"
  }
}